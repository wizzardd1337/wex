-- Вставь сюда ключ, который ты вводишь в executor
local userKey = "KEY123"  -- Убедись, что ключ без пробелов, правильно прописан

-- Таблица ключей с временем действия в минутах
local keys = {
    ["WexcoreTest"] = 1,    -- 60 минут
    ["KEY456"] = 1440,  -- 1 день (1440 минут)
}

-- Функция для проверки ключа
local function isKeyValid(key)
    -- Проверяем, есть ли ключ в таблице
    local validMinutes = keys[key]
    if not validMinutes then
        return false, "Ключ не найден"  -- Если ключа нет в таблице
    end

    local currentTime = os.time()

    -- Если активация не была записана, запишем текущее время
    if not _G.activationTime then
        _G.activationTime = currentTime
    end

    -- Считаем прошедшее время в минутах
    local elapsed = (currentTime - _G.activationTime) / 60

    -- Проверка, не истёк ли срок действия
    if elapsed > validMinutes then
        return false, "Срок действия ключа истёк"
    end

    -- Если ключ валиден, возвращаем оставшееся время
    return true, math.floor(validMinutes - elapsed)
end

-- Проверка ключа
local valid, info = isKeyValid(userKey)

-- Если ключ валиден, запускаем скрипт
if valid then
    print("Ключ валиден, осталось минут: "..info)
    -- Запуск твоего скрипта (заменить на нужный тебе loadstring)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/wizzardd1337/adminguii/main/admingui", true))()
else
    -- Если ключ невалиден или срок действия истёк, выводим ошибку
    warn("Ошибка активации: "..info)
end
